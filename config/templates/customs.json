{
    "index_patterns": ["custom-*"],
    "settings": {
        "number_of_shards": 1,
        "number_of_replicas": 0
    },
    "mappings": {
        "dynamic": true,
        "properties": {
            "timestamp": {
                "type": "date"
            },
            "content": {
                "type": "text"
            }
        }
    }
}